<UserControl x:Class="cpapviewer.Controls.OximetrySummary"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:cpapviewer.Controls"
             xmlns:ui="http://schemas.modernwpf.com/2019"
             mc:Ignorable="d"
             d:DesignHeight="800" d:DesignWidth="350">
    <Button Name="btnImportOxy" HorizontalAlignment="Center" Style="{StaticResource AccentButtonStyle}" ClickMode="Press" Margin="20" Click="BtnImportOxy_OnClick">
        <StackPanel Orientation="Horizontal">
            <ui:FontIcon Glyph="&#xE898;" FontSize="14" Margin="0 0 10 0"/>
            <TextBlock>Import Pulse Oximeter Data</TextBlock>
        </StackPanel>
    </Button>
</UserControl>
